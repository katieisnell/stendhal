<?xml version="1.0" encoding="UTF-8"?>
<actions xmlns="stendhal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="stendhal ../actions.xsd ">
	
	<action name="listproducers">
		<implementation class-name="games.stendhal.client.actions.DefaultAction"/>
		<type value="listproducers"/>
		<number_of_parameters>
			<minimum value="0" />
			<maximum value="0" />
		</number_of_parameters>
	</action>
	
	<action name="progressstatus">
		<implementation class-name="games.stendhal.client.actions.DefaultAction"/>
		<type value="progressstatus"/>
		<number_of_parameters>
			<minimum value="0" />
			<maximum value="0" />
		</number_of_parameters>
	</action>
	
	<action name="where">
		<implementation class-name="games.stendhal.client.actions.DefaultAction"/>
		<type value="where"/>
		<remainder key="target" canBeEmpty="false"/>
		<number_of_parameters>
			<minimum value="0" />
			<maximum value="0" />
		</number_of_parameters>
	</action>
	
	<action name="who">
		<implementation class-name="games.stendhal.client.actions.DefaultAction"/>
		<type value="who"/>
		<number_of_parameters>
			<minimum value="0" />
			<maximum value="0" />
		</number_of_parameters>
	</action>
	
</actions>